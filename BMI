#include <bits/stdc++.h>
using namespace std;

int main() 
{
	
    cin.tie(NULL);
    ios::sync_with_stdio(false);
   int t;
   cin>>t;
   while(t--)
   {
   	int m,h;
   	cin>>m>>h;
   	int bmi;
   	h=pow(h,2);
   	bmi=m/h;
   	if(bmi<=18)
   	cout<<"1";
   	else if(bmi<=24)
   	cout<<"2";
   	else if(bmi<=29)
   	cout<<"3";
   	else
   	cout<<"4";
   	cout<<endl;
   }
    return 0;
}
