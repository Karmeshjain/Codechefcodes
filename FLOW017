#include <iostream>
using namespace std;
int main(void) 
{
	int t;
	cin>>t;
	while(t--)
	{
	int a,b,c,max;
	scanf("%d %d %d",&a,&b,&c);
	max=a>b?(a>c?a:c):(b>c?b:c);
    if(max==a)
    {
    	cout<<(b>c?b:c);
	}else if(max==b)
	{
		cout<<(a>c?a:c);
	}else 
	{
		cout<<(a>b?a:b);
	}
	printf("\n");
      }
	return 0;
}

