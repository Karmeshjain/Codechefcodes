#include <bits/stdc++.h>//KarmeshDuggar
using namespace std;
#define ll long long

int main() 
{
    cin.tie(NULL);
    ios::sync_with_stdio(false);
   int n,h,x;
   cin>>n>>h>>x;
   int val,flag=0;
   for(int i=0;i<n;i++)
   {
   	cin>>val;
   	if(x+val>=h)
   	flag=1;
   }
   if(flag==1)
   cout<<"YES";
   else
   cout<<"NO";
   
    return 0;
}
